<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />
            <corners android:radius="3000dp" />
            <stroke
                android:width="6dp"
                android:color="@color/teal" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="3000dp" />
            <solid
                android:width="1dp"
                android:color="@color/green" />
        </shape>
    </item>

</layer-list>